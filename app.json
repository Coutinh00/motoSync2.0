{
  "expo": {
    "name": "MotoSync 2.0",
    "slug": "motosync",
    "version": "3.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "description": "Sistema de gestão de frotas de motocicletas para Mottu",
    "keywords": [
      "mottu",
      "motos",
      "gestão",
      "frota",
      "mobile"
    ],
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "githubUrl": "https://github.com/seu-usuario/motosync2.0",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mottu.motosync",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Este app precisa acessar a câmera para fotografar motos.",
        "NSLocationWhenInUseUsageDescription": "Este app precisa acessar a localização para rastrear motos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "package": "com.mottu.motosync",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "051321b4-c8a8-4956-a598-85618b18181c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/051321b4-c8a8-4956-a598-85618b18181c"
    }
  }
}
